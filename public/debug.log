                INNER JOIN orden_detalle od  ON od.id_pedido = pe.id_pedido_ejecutivo
                WHERE od.id_producto IN (SELECT DISTINCT id_producto FROM productos )    GROUP BY pe.id_pedido_ejecutivo 
                    ORDER BY pe.fecha DESC LIMIT :offset, :porPagina
