                pe.id_pedido_ejecutivo,
                c.nombre_cliente,
                c.encargado,
                e.id_ejecutivo,
                e.nombre_vendedor,
                pe.id_proforma,
                pe.tipo_venta,
                pe.fecha,                
                pe.fecha_entrega,
                pe.id_repartidor,
                IFNULL(pe.nombre_cliente,'--') AS nota,
                pe.id_estado ,
                os.comentario,
                pe.comentarios AS comentario_vendedor,
