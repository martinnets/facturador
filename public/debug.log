                c.id_cliente,
                tp.nombre_tipo_pago,
                od.tipo_precio,
                sum(od.cantidad*od.precio) total
                FROM orden pe
                INNER JOIN clientes c ON pe.id_cliente = c.id_cliente
                INNER JOIN ejecutivos e ON pe.id_ejecutivo = e.id_ejecutivo
                INNER JOIN tipo_pago tp ON pe.id_tipo_pago = tp.id_tipo_pago
